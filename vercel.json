{
  "rewrites": [
    {
      "source": "/api/heygen/:path*",
      "destination": "https://api.heygen.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Permissions-Policy", "value": "microphone=(\"*\"), camera=(\"*\")" }
      ]
    }
  ]
}
  